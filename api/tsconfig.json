{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "pages/_app.jsx",
    "components/Dropdowns/UserDropdown.jsx",
    "components/Dropdowns/TableDropdown.jsx",
    "components/Dropdowns/PagesDropdown.jsx",
    "components/Dropdowns/NotificationDropdown.jsx",
    "components/Dropdowns/IndexDropdown.jsx",
    "components/Cards/CardLineChart.jsx",
    "components/Cards/CardBarChart.jsx",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/Cards/ArticleTextEditor.jsx"
  ],
  "exclude": ["node_modules"]
}
